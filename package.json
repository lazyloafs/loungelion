    "url-loader": "^4.1.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongojs": "^3.1.0",
    "mongoose": "^5.10.1",
    "node-geocoder": "^3.27.0",
    "react-bootstrap": "^1.3.0",
    "react-map-gl": "^5.2.7"
  }
